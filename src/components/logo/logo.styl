.logo
  padding-left 19px
  margin-bottom 85px
  @media only screen and (max-width 1280px)
    margin-bottom 10px
  @media only screen and (min-width 1631px)
    margin-bottom 120px
  &__circle
    margin 20px auto 31px auto
    width 254px
    height 253px
    border 1px solid #3d6cb0
    border-radius 253px
    @media only screen and (max-width 1280px)
      margin 0 auto
    @media only screen and (min-width 1631px)
      margin 59px auto 37px auto
      width 356px
      height 357px
  &__round
    margin 15px
    width 222px
    height 222px
    background-color #3d6cb0
    border-radius 222px
    @media only screen and (min-width 1631px)
      margin 20px
      width 316px
      height 316px