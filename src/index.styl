html
  font-size 16px
  color black
  background-color white
  font-family MyriadPro, Helvetica, Arial, sans-serif
  font-weight 500
  font-style normal

.left-column
  width 690px
  margin 0 auto
  @media only screen and (min-width 1280px)
    width 34.11%
    margin 69px 0 0 59px
    float left
  @media only screen and (min-width 1631px)
    margin-left 86px

.right-column
  width 690px
  margin 0 auto
  @media only screen and (min-width 1280px)
    width 53%
    margin 75px 0 0 84px
    float left
  @media only screen and (min-width 1631px)
    margin 109px 0 0 116px