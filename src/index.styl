html
  font-size 16px
  color black
  background-color white
  font-family myriad_pro, Helvetica, Arial, sans-serif

div
  display block

.left-column
  width 34.11%
  margin 69px 0 0 59px
  float left

@media only screen and (max-width 1280px)
  .left-column
    width 42.5rem
    float none
    margin 0 auto

@media only screen and (min-width 1631px)
  .left-column
    margin-left 86px

.right-column
  width 53%
  margin 75px 0 0 84px
  float left

@media only screen and (max-width 1280px)
  .right-column
    width 42.5rem
    float none
    margin 0 auto

@media only screen and (min-width 1631px)
  .right-column
    margin 109px 0 0 108px