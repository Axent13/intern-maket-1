doctype html
html
    head
        meta(charset="utf-8")
        meta(name="keywords" content="resume cv template")
        meta(name="description" content="A template of resume")
        meta(name="viewport" content="initial-scale=1.0, width=device-width")
        title Resume
        link(rel="shortcut icon" href="favicon.ico" type="image/x-icon")
    body
        .left-column
            include components/logo/logo.pug
            include components/profile/profile.pug
            include components/contact/contact.pug
            include components/skills/skills.pug
        .right-column
            include components/education/education.pug
            include components/experience/experience.pug
            include components/software/software.pug